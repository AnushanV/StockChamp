				p(id="stock1" class="navbar-item" action="/loadStock1" method="post")
					a= stock1
				p(id="stock2" class="navbar-item" href="/")
					a= stock2
				p(id="stock3" class="navbar-item" href="/") 
					a= stock3
					
			div(class="navbar-end")
